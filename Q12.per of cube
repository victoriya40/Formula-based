import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Side: ");
		int a=sc.nextInt();
		 int result1=6*a*a;
		 int result2=a*a;
		 int result3=12*a;
		 System.out.println("Surface of cube: "+result1); 
		 System.out.println("perimeter of volume: "+result2);
		 	System.out.println("Perimeter of cube: "+result3);
		 
	}
}
